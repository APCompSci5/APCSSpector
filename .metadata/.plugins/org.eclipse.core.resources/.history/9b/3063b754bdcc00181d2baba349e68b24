package ch5;

public class LoopingChallenge {
	public static void main(String[] args){
		//1. Create a program that prints the sum from 1 to 1000.
		for(int k=0;k<=10000;k++){
			System.out.println(k + k);
		}
		System.out.println();
		for(int x=0;x<=100;x++){
			System.out.println(x + 2);
		}
		
	}

}
