import java.awt.Color;
import java.awt.Graphics;

public class Box {
	private Color boxColor;
	public Box(Graphics g ,Color cc, int x) {
		
		{
			boxColor = cc;
			g.setColor(boxColor);
			g.fillRect(x,250,300,400);
			g.setColor(Color.black);
			g.fillOval(x+5,325,50,50);
			g.fillOval(x+95,325,50,50);
			
		}
		
	}

}
