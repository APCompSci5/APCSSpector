
public class Q1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
        System.out.println(Multiples(5));
        		
        }
	
public static int Multiples(int sum){
	 for(int i=1; i<=1000;i++){
      sum=0;
     	if(i%3==0 || i%5==0)
     		sum++;


}
	 return sum ;
}
}

