
public class Q1 {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
        System.out.println(Multiples());
        		
        }
	
public static int Multiples(){
	  int sum=0;
	  int num=0;
	  int hum=0;
	  int pum=0;
	 for(int i=1; i<=10;i++){
		 if(i% 3==0 && i%5!=0)
				sum++;
			else if(i % 5==0 && i%3!=0)
				num++;
			else if(i % 3==0 && i % 5 ==0)
			pum++;
			else{} 
   hum=sum+num+pum;
}
	 return hum ;
}
}

