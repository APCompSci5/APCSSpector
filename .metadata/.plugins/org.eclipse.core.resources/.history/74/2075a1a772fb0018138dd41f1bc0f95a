import java.awt.Color;
import java.awt.Graphics;

class Smoke extends TrainCar{
	private int x;
	public Smoke(Graphics g, int x1) {
		super(g, Color.gray, x1);
		x= x1;
		drawSmoke(g);
		drawTime(g);
		
	
	}
	private void drawTime(Graphics g) {
		// TODO Auto-generated method stub
		
	}
	private void drawSmoke(Graphics g) {
		g.drawOval(x+100, 200, 50, 50);
		g.fillOval(x+100, 200, 50, 50);
		
	}
}
