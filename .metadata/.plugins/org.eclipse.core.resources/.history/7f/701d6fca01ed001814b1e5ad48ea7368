
public class Pumpkin {
	private double weight;
	private double pricePerPound=.25;
	private double growthrate=.5;
	
	public  Pumpkin(double pounds){
		weight = pounds;
		
	}
   public void setWeight(double w){
	   if(w < 100)
	   weight = w;
}
  public double getWeight(){
	  return weight;
	
}
   public void grow(double g,int num){
	   g= growthrate;
	   num = 5;
	   weight = g * num * weight;
	   
	   
}
   public double Price(){
	   weight=weight * pricePerPound;
	   return weight;
   }
   
}