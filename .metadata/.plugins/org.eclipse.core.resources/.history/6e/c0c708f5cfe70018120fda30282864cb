	public bigdiff(int[] nums) {
		nums = new int[4];
		
		for(int i=0; i<nums.length;i++){
			nums[i]= Math.max(nums.length, i)-Math.min(nums.length,i);
			System.out.println(nums[i]);
	}
		return;
		  
			
		  }
				
	
		
	}
		 
