
public class Q1 {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
        System.out.println(Multiples());
        		
        }
	
public static int Multiples(){
	  int sum=0;
	  int num=0;
	  int total=0;
	 for(int i=1; i<=10;i++){
		 if(i % 3==0)
		       sum=sum+3;
		  if(i % 5==0)
			  num=num+5;
		 
		  total=sum+num;
			  
		 }

	 return total;
}
}

