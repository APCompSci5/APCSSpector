
public class Q1 {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
        Multiples();
        		
        }
	
public static int Multiples(){
	  int sum=0;
	 for(int i=1; i<10;i++){
     	if(i%3==0 || i%5==0)
     		sum++;
    System.out.println(sum);


}
	 return sum ;
}
}

